							<td class="data-tbl-label data-score"> Score: </td> <td class="data-score"> <%=item.score%> </td>
						</tr>
						<tr>
							<td class="data-tbl-label"> isMeta: </td> <td> <%=item.isMeta%> </td>
						</tr>
						<tr>
							<td class="data-tbl-label"> src: </td> 
							<td>
								<a href="<%=item.src%>" target="_blank"> <%=item.src%> </a>
							</td>		
						</tr>
						<tr>
							<td class="data-tbl-label"> alt: </td> <td> <%=item.attribs.alt%> </td>
						</tr>
						<tr>
							<td class="data-tbl-label"> title: </td> <td> <%=item.attribs.title%> </td>
						</tr>
						<tr>
							<td class="data-tbl-label"> class: </td> <td> <%=item.attribs.class%> </td>
						</tr>

					</table>
				</td>
			</tr>
		<% }} %>	



	</table>
</div>
