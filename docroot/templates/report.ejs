					<a href="<%=item.src%>" target="_blank">
						<img class="table-image" src="<%=item.src%>"></img> </td>
					</a>	
				<td class="spacer-column"></td>
				<td class="rpt-data-cell"> 
					<table id="subtable2-<%=i%>">
